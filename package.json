{
  "name": "preschian-com",
  "version": "1.0.0",
  "description": "Revamp my blog using ReactJS",
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/preschian/preschian-com.git"
  },
  "scripts": {
    "start": "gulp",
    "build": "gulp build",
    "sass-lint": "sass-lint build/**/*.scss -v -q",
    "post": "node build/node/post-article",
    "deploy": "npm run post && npm run build && firebase deploy --only hosting"
  },
  "author": "Preschian Febryantara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/preschian/preschian-com/issues"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "axios": "0.15.3",
    "babel-core": "^6.17.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "bourbon": "^4.2.7",
    "browser-sync": "^2.14.0",
    "connect-history-api-fallback": "^1.3.0",
    "dotenv-webpack": "^1.3.1",
    "eslint": "^3.14.1",
    "eslint-plugin-react": "^6.9.0",
    "firebase": "^3.6.9",
    "flexiblegs-scss": "^5.5.2",
    "gulp": "^3.9.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-processhtml": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-uglify": "^2.0.1",
    "gulp-watch": "^4.3.8",
    "json-loader": "^0.5.4",
    "live-server": "^1.0.0",
    "mobx": "^3.0.2",
    "mobx-react": "^4.1.0",
    "moment": "^2.17.1",
    "normalize.css": "5.0.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-helmet": "^4.0.0",
    "react-router": "^3.0.2",
    "run-sequence": "^1.2.1",
    "sass-lint": "^1.9.1",
    "webpack": "^1.14.0",
    "webpack-stream": "^3.2.0",
    "wpapi": "^1.0.2",
    "write-json-file": "^2.0.0"
  }
}
