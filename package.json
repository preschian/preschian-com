{
  "name": "preschian-com",
  "description": "Preschian Febryantara Blog",
  "version": "2.0.0",
  "author": "Preschian Febryantara",
  "keywords": ["gatsby", "graphql", "reactjs"],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "clean": "rm -rf .cache && rm -rf ./public && rm -rf ./preschian-com",
    "build": "yarn run clean && gatsby build",
    "develop": "gatsby develop",
    "deploy": "yarn run build && mv ./public ./preschian-com && now ./preschian-com && now alias"
  },
  "now": {
    "alias": "preschian.com"
  },
  "babel": {
    "presets": ["react", "env"],
    "plugins": ["emotion", "transform-decorators-legacy"]
  },
  "dependencies": {
    "babel-eslint": "^8.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "date-fns": "^1.29.0",
    "emotion": "^8.0.5",
    "emotion-server": "^8.0.5",
    "eslint": "^4.12.1",
    "eslint-plugin-react": "^7.5.1",
    "gatsby": "^1.9.45",
    "gatsby-link": "^1.6.17",
    "gatsby-plugin-emotion": "^1.1.8",
    "gatsby-plugin-react-helmet": "^1.0.5",
    "gatsby-plugin-sharp": "^1.6.22",
    "gatsby-remark-copy-linked-files": "^1.5.22",
    "gatsby-remark-images": "^1.5.33",
    "gatsby-remark-prismjs": "^1.2.10",
    "gatsby-source-filesystem": "^1.5.9",
    "gatsby-transformer-remark": "^1.7.23",
    "gatsby-transformer-sharp": "^1.6.15",
    "local-storage": "^1.4.2",
    "mobx": "^3.3.2",
    "mobx-react": "^4.3.5",
    "react-emotion": "^8.0.5"
  },
  "devDependencies": {
    "prettier": "^1.6.1"
  }
}
